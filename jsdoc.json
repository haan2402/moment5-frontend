{
    "source": {
        "include": ["src"],
        "includePattern": ".js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "sourceType": "module",
    "opts": {
        "destination": "./docs"
    }
}